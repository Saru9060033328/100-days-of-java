class  loopParameter
{
	static void print1(int start,int end){
		for(int i=start;i<=end;i++){
			System.out.println(i+"");
		}
	}
	static void print2(int end,int start){
		for(int i=end;i>=start;i--){
			System.out.println(i+"");
		}
	}
	static void print3(int start,int end){
		for(int i=start;i<=end;i++){
			if(i<=3){
				System.out.println(i);
			}
		}
	}
	public static void main(String[] args) 
	{
		print1(1,6);
		print2(6,1);
		print3(1,3);
	}
}
